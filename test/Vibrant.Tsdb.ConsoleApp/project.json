{
   "version": "1.0.0-*",
   "buildOptions": {
      "emitEntryPoint": true,
      "copyToOutput": {
         "include": [
            "appsettings.*"
         ]
      }
   },

   "dependencies": {
      "Microsoft.Extensions.Configuration": "1.0.0",
      "Microsoft.Extensions.Configuration.Json": "1.0.0",
      "Vibrant.Tsdb": "4.0.0-*",
      "Vibrant.Tsdb.Ats": "4.0.0-*",
      "Vibrant.Tsdb.Files": "4.0.1-*",
      "Vibrant.Tsdb.InfluxDB": "4.0.0-*",
      "Vibrant.Tsdb.Redis": "4.0.0-*",
      "Vibrant.Tsdb.Sql": "4.0.0-*"
   },

   "frameworks": {
      "net46": {
         "buildOptions": {
            "define": []
         }
      },
      "netcoreapp1.0": {
         "buildOptions": {
            "define": ["COREFX"]
         },
         "imports": [
            "dnxcore50",
            "portable-net451+win8"
         ],
         "dependencies": {
            "Microsoft.NETCore.App": {
               "type": "platform",
               "version": "1.0.0"
            }
         }
      }
   }
}
